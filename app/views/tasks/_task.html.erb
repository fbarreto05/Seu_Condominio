<div id="<%= dom_id task %>">
  <p>
    <strong>Name:</strong>
    <%= task.name %>
  </p>

  <p>
    <strong>Beginning:</strong>
    <%= task.beginning %>
  </p>

  <p>
    <strong>End:</strong>
    <%= task.end %>
  </p>

  <p>
    <strong>Estimated cost:</strong>
    <%= task.estimated_cost %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= task.status %>
  </p>

</div>
